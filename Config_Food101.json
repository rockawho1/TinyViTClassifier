{
  "model": {
    "name": "tinyvit",
    "img_size": 384,
    "in_chans": 3,
    "num_classes": 101,
    "embed_dim": 64,
    "patch_size": 24,
    "block_depth": 6,
    "num_heads": 4,
    "window_sizes": [7, 7, 14, 7],
    "mlp_ratio": 2.0,
    "drop": 0.0,
    "mlp_drop": 0.0,
    "proj_drop": 0.0,
    "attn_drop": 0.0,
    "lin_type": "linear"
  },
  "data": {
    "dataset": "Food101",
    "data_path": "./data",
    "num_workers": 4,
    "augmentation": true
  },
  "experiment": {
    "experiment_name": "food_test_01",
    "return_attn": true,
    "seed": 42,
    "profile": false,
    "log_per_class_acc": false
  },
  "training": {
    "batch_size": 128,
    "epochs": 50,
    "learning_rate": 3e-4,
    "weight_decay": 0.05,
    "print_every_k": 50,
    "save_every_k": 10
  }
}
